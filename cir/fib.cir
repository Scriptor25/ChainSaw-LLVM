def num fib num n
	%0  call -   %n const num 1
	%1  call fib %0
	%2  call -   %n const num 2
	%3  call fib %2
	%4  call +   %1 %3
	%5  call ==  %n const num 1
	%6  call ?   %5 const num 1 %4
	%7  call ==  %n const num 0
	ret call ?   %7 const num 0 %6
enddef

def num main
	ret call fib const num 10
enddef
